{
	"compilerOptions": {
		"target": "ES2022",
		"module": "commonjs",
		"noEmit": true,
		"allowJs": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"useUnknownInCatchVariables": false
	},
	"include": [
		"index.js"
	],
	"exclude": [
		"./node_modules"
	],
	"types": [
		"node"
	]
}
